{
    "name": "styleci/styleci",
    "description": "StyleCI Is The PHP Coding Style Continuous Integration Service",
    "keywords": ["StyleCI", "CS", "Laravel", "Alt Three"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "~5.1.28",
        "alt-three/logger": "^1.1",
        "alt-three/validator": "^1.4",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^3.0",
        "graham-campbell/exceptions": "^7.0",
        "graham-campbell/core": "^4.2",
        "graham-campbell/github": "^4.2",
        "graham-campbell/htmlmin": "^4.2",
        "guzzlehttp/guzzle": "^6.1",
        "mccool/laravel-auto-presenter": "^4.2",
        "styleci/fixer": "^5.0",
        "styleci/login": "^1.4",
        "styleci/storage": "^2.1",
        "vinkla/hashids": "^2.2",
        "vinkla/pusher": "^2.2"
    },
    "require-dev": {
        "alt-three/testbench": "^1.1",
        "filp/whoops": "^1.1",
        "graham-campbell/testbench-core": "^1.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^5.1.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "StyleCI\\StyleCI\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StyleCI\\Tests\\StyleCI\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize --force",
            "php artisan config:cache",
            "php artisan route:cache",
            "chmod -R 755 storage"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize --force",
            "php artisan config:cache",
            "php artisan route:cache",
            "chmod -R 755 storage"
        ]
    },
    "config": {
        "platform": {
            "php": "5.6.4"
        },
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
